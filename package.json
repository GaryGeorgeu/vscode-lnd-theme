{
	"name": "lnd-theme",
	"displayName": "lnd-theme",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
    "Snippets",
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:typescript"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
    ],
    "snippets": [
      {
        "language": "typescript",
        "path": "./snippets/global.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/global.json"
      },
      {
        "language": "css",
        "path": "./snippets/global.json"
      },
      {
        "language": "styled-css",
        "path": "./snippets/global.json"
      }
    ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"@lessondesk/theme": "^1.0.0",
		"color": "^3.1.2"
	}
}
